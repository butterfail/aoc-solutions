{
  "name": "advent-of-code",
  "author": "Gr√©goire Ciles",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "aoc": "node --experimental-specifier-resolution=node --loader ts-node/esm ./aoc.ts",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format:write": "prettier --write \"./**/*.{js,jsx,ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,mdx}\" --cache"
  },
  "devDependencies": {
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "chalk": "5.3.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "dotenv": "16.3.1"
  }
}
