{
  "compilerOptions": {
    "strict": false,
    "composite": false,
    "module": "commonjs",
    "target": "esNext",
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "strictNullChecks": false,
    "skipLibCheck": false
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
